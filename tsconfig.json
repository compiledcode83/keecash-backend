{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@api": ["apps/api/src"],
      "@api/*": ["apps/api/src/*"],
      "@web": ["apps/web/src"],
      "@web/*": ["apps/web/src/*"],
      "@admin": ["apps/admin/src"],
      "@admin/*": ["apps/admin/src/*"],
      "@producer": ["apps/producer/src"],
      "@producer/*": ["apps/producer/src/*"],
      "@consumer": ["apps/consumer/src"],
      "@consumer/*": ["apps/consumer/src/*"],
      "@app/beneficiary-user": ["libs/beneficiary-user/src"],
      "@app/beneficiary-user/*": ["libs/beneficiary-user/src/*"],
      "@app/beneficiary-wallet": ["libs/beneficiary-wallet/src"],
      "@app/beneficiary-wallet/*": ["libs/beneficiary-wallet/src/*"],
      "@app/bridgecard": ["libs/bridgecard/src"],
      "@app/bridgecard/*": ["libs/bridgecard/src/*"],
      "@app/card": ["libs/card/src"],
      "@app/card/*": ["libs/card/src/*"],
      "@app/cipher-token": ["libs/cipher-token/src"],
      "@app/cipher-token/*": ["libs/cipher-token/src/*"],
      "@app/closure-reason": ["libs/closure-reason/src"],
      "@app/closure-reason/*": ["libs/closure-reason/src/*"],
      "@app/common": ["libs/common/src"],
      "@app/common/*": ["libs/common/src/*"],
      "@app/country": ["libs/country/src"],
      "@app/country/*": ["libs/country/src/*"],
      "@app/country-activation": ["libs/country-activation/src"],
      "@app/country-activation/*": ["libs/country-activation/src/*"],
      "@app/database": ["libs/database/src"],
      "@app/database/*": ["libs/database/src/*"],
      "@app/document": ["libs/document/src"],
      "@app/document/*": ["libs/document/src/*"],
      "@app/enterprise-profile": ["libs/enterprise-profile/src"],
      "@app/enterprise-profile/*": ["libs/enterprise-profile/src/*"],
      "@app/env": ["libs/env/src"],
      "@app/env/*": ["libs/env/src/*"],
      "@app/notification": ["libs/notification/src"],
      "@app/notification/*": ["libs/notification/src/*"],
      "@app/person-profile": ["libs/person-profile/src"],
      "@app/person-profile/*": ["libs/person-profile/src/*"],
      "@app/pricing": ["libs/pricing/src"],
      "@app/pricing/*": ["libs/pricing/src/*"],
      "@app/shareholder": ["libs/shareholder/src"],
      "@app/shareholder/*": ["libs/shareholder/src/*"],
      "@app/sumsub": ["libs/sumsub/src"],
      "@app/sumsub/*": ["libs/sumsub/src/*"],
      "@app/transaction": ["libs/transaction/src"],
      "@app/transaction/*": ["libs/transaction/src/*"],
      "@app/triple-a": ["libs/triple-a/src"],
      "@app/triple-a/*": ["libs/triple-a/src/*"],
      "@app/twilio": ["libs/twilio/src"],
      "@app/twilio/*": ["libs/twilio/src/*"],
      "@app/user": ["libs/user/src"],
      "@app/user/*": ["libs/user/src/*"]
    },
    "resolveJsonModule": true
  },
  "ts-node": {
    "require": ["tsconfig-paths/register"]
  },
  "exclude": ["node_modules", "dist", "**/*.d.ts"]
}
